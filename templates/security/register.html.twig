{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        <button type="submit">S'inscrire</button>
    {{ form_end(form) }}

    <a href="{{ path('app_login') }}">Déjà un compte ? Connectez-vous ici.</a>
{% endblock %}
